        </geometry>
      </visual>
      <collision name="stick_D_collision">
        <pose>1 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>2 0.1 0.1</size>
          </box>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.05</mu>
              <mu2>0.05</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <joint name="base_link_to_stick_A" type="revolute">
      <parent>base_link</parent>
      <child>stick_A</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <dynamics/>
      </axis>
    </joint>

    <joint name="stick_A_to_stick_B" type="revolute">
      <parent>stick_A</parent>
      <child>stick_B</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <dynamics/>
      </axis>
    </joint>

    <joint name="stick_B_to_stick_C" type="revolute">
      <parent>stick_B</parent>
      <child>stick_C</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <dynamics/>
      </axis>
    </joint>

    <joint name="stick_C_to_stick_D" type="revolute">
      <parent>stick_C</parent>
      <child>stick_D</child>
      <pose>0 0 0 0 0 0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <dynamics/>
      </axis>
    </joint>

    <!-- gazebo_ros_control plugins -->
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>demo_model</robotNamespace>
      <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
      <legacyModeNS>true</legacyModeNS>
    </plugin>

    <plugin name="joint_state_publisher" filename="libgazebo_ros_joint_state_publisher.so">
      <robotNamespace>demo_model</robotNamespace>
      <jointName>stick_A_to_stick_B, stick_B_to_stick_C, stick_C_to_stick_D</jointName>
      <!-- Issue : base_link_to_stick_A を追加したらエラーが出る -->
      <!-- Ignoring transform for child_frame_id "stick_A" from authority "unknown_publisher" because of an invalid quaternion in the transform (nan nan nan nan) -->
    </plugin>

  </model>
</sdf>